{
  "links": [
    {
      "selector": ".nav-item:not(.active)",
      "match": "text",
      "mapping": {
        "Stash": "stash",
        "Cherry-pick": "cherry-pick",
        "Rebase": "rebase",
        "Files": "file-tree",
        "GitHub": "hosting",
        "Submodules": "submodules",
        "Worktrees": "worktrees"
      }
    },
    {
      "selector": ".toolbar-btn",
      "match": "title",
      "mapping": {
        "Tags": "tags",
        "Remote": "remote"
      }
    },
    {
      "selector": ".settings-nav-item",
      "match": "text",
      "mapping": {
        "Appearance": "settings-appearance",
        "Editor": "settings-editor",
        "Git Config": "settings-gitconfig",
        "AI Settings": "settings-ai",
        "Keybindings": "settings-keybindings",
        "External Tools": "settings-tools"
      }
    },
    {
      "selector": ".titlebar-btn[title='Settings']",
      "match": "title",
      "target": "settings-appearance"
    },
    {
      "selector": ".tab-add",
      "match": "text",
      "target": "open-repository"
    },
    {
      "selector": ".newtab-actions .btn-secondary",
      "match": "text",
      "mapping": {
        "Init": "init"
      }
    },
    {
      "selector": ".branch-selector",
      "match": "text",
      "target": "branches-dialog"
    },
    {
      "selector": ".view-toggle .toggle-btn",
      "match": "text",
      "mapping": {
        "Split": "changes-split",
        "Inline": "changes"
      }
    },
    {
      "selector": ".fh-diff-toggle .toggle-btn",
      "match": "text",
      "mapping": {
        "Split": "file-history-split",
        "Inline": "file-history"
      }
    },
    {
      "selector": ".conflict-detail-actions .btn-primary",
      "match": "text",
      "mapping": {
        "3-way Merge Viewer": "merge-viewer"
      }
    },
    {
      "selector": ".branch-detail-actions .btn-secondary",
      "match": "text",
      "mapping": {
        "Merge": "branch-merge"
      }
    },
    {
      "selector": ".page-actions .btn-secondary",
      "match": "text",
      "mapping": {
        "+ New Branch": "branch-create"
      }
    },
    {
      "selector": ".operation-buttons .btn-secondary",
      "match": "text",
      "mapping": {
        "Rename": "branch-rename"
      }
    },
    {
      "selector": ".operation-buttons .btn-danger",
      "match": "text",
      "mapping": {
        "Delete": "branch-delete"
      }
    },
    {
      "selector": ".operation-footer .btn-secondary",
      "match": "text",
      "mapping": {
        "Rebase": "conflict"
      }
    },
    {
      "selector": ".operation-footer .btn-primary",
      "match": "text",
      "mapping": {
        "Execute Cherry-pick": "conflict",
        "Execute Revert": "conflict"
      }
    },
    {
      "selector": ".operation-footer .btn-warning",
      "match": "text",
      "mapping": {
        "Start Rebase": "conflict"
      }
    },
    {
      "selector": ".icon-btn[title='File History']",
      "match": "title",
      "target": "file-history"
    },
    {
      "selector": ".icon-btn[title='Blame']",
      "match": "title",
      "target": "blame"
    },
    {
      "selector": ".hunk-btn",
      "match": "text",
      "mapping": {
        "Discard": "discard-changes"
      }
    },
    {
      "selector": ".filter-toggle",
      "match": "text",
      "target": "history-filter"
    },
    {
      "selector": ".hosting-actions .btn-primary",
      "match": "text",
      "mapping": {
        "Create PR": "hosting-create-pr"
      }
    },
    {
      "selector": ".hosting-footer-actions .btn-primary",
      "match": "text",
      "mapping": {
        "Checkout PR": "hosting-pr-review"
      }
    },
    {
      "selector": ".review-back-btn",
      "match": "title",
      "target": "hosting"
    },
    {
      "selector": ".hosting-tab",
      "match": "text",
      "mapping": {
        "Pull Requests": "hosting",
        "Issues": "hosting-issues",
        "CI/CD": "hosting-cicd"
      }
    }
  ],
  "modals": [
    "tags",
    "branches-dialog",
    "open-repository",
    "init",
    "settings-appearance",
    "settings-editor",
    "settings-gitconfig",
    "settings-ai",
    "settings-keybindings",
    "settings-tools",
    "ai-commit",
    "remote",
    "conflict",
    "merge-viewer",
    "search",
    "discard-changes",
    "hosting-create-pr",
    "branch-create",
    "branch-rename",
    "branch-delete",
    "branch-merge"
  ],
  "close": [
    ".modal-close",
    {
      "selector": ".modal-footer .btn-secondary",
      "match": "text",
      "value": "Close"
    },
    {
      "selector": ".modal-footer .btn-secondary",
      "match": "text",
      "value": "Cancel"
    }
  ],
  "ignore": ["mockup"]
}
